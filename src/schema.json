{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "Process execution commands",
  "description": "Execute commands in array one by one.",
  "type": "object",
  "properties": {
    "array_command": {
      "type": "array",
          "items": {
            "type": "string",
            "minLength": 1
          },
          "minItems": 1
    }
  },
  "additionalProperties": false,
  "required": ["array_command"]
}